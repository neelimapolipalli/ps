#include<stdio.h>
main()
{
 int a[10],i,j,flag,n;
 printf("ENTER SIZE OF ARRAY");
 scanf("%d",&n);
 for(i=0;i<n;i++)
 {
     scanf("%d",&a[i]);
 }

for(i=0;i<n;i++)
{
    flag=0;

    for(j=2;j<a[i];j++)
    {
        if(a[i]%j==0)
        {
            flag=1;
            break;
            
        
    }
}

if(flag==0)
{
 printf("%d \t",a[i]);
}
}
}

