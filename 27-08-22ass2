#include <stdio.h>
int main()
{
  char str[]="abcd";
  subString(str,strlen(str));
  return 0;
}
void subString(char str[],int n)
{
  int x,i,j,k;
  for(i=1;i<=n;i++)
  {
    for(j=0;j<=n-i;j++)
    {
      int x=j+i;
      for(k=j;k<x;k++)
      {
        printf("%c",str[k]);
        }
        
        printf("\n");
      }
    
  }
}
