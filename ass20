li = []
n=int(input("enter how many into list:"))
for ele in range(n):
    num=int(input())
    li.append(num)
print(li)
prime=[]
for i in li:
    c=0
    for j in range(1,i):
        if i%j==0:
            c+=1
    if c==1:
        prime.append(i)
print(prime)
prime.sort()
print("maximum prime",prime[-1])
print("minimum prime",prime[0])

